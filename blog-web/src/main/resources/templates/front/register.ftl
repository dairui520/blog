<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
            <h4 class="modal-title" id="myModalLabel">Register</h4>
        </div>
        <div class="modal-body">
            <form role="form" id="modalForm">
                <div class="form-group">
                    <label for="account" class="control-label">email</label>
                    <input type="text" class="form-control" id="account" name="account">
                </div>
                <div class="form-group">
                    <label for="password" class="control-label">password</label>
                    <input type="password" class="form-control" id="password" name="password">
                </div>
                 <div class="form-group">
                     <label for="name" class="control-label">nickName</label>
                     <input type="text" class="form-control" id="name" name="name">
                 </div>
                 <div class="form-group">
                     <label for="homeUrl" class="control-label">home</label>
                     <input type="text" class="form-control" id="homeUrl" name="homeUrl">
                 </div>
                 <div class="form-group">
                     <label for="head" class="control-label">head</label>
                     <input type="text" class="form-control" id="head" name="head">
                 </div>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" id="registerBtn">register</button>
        </div>
    </div>
</div>